{
  "name": "counter-app",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/compact"
  ],
  "scripts": {
    "compact": "turbo run compact",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "counter-cli-remote": "npm --workspace=@midnight-ntwrk/counter-cli run start-testnet-remote",
    "counter-cli-remote-ps": "npm --workspace=@midnight-ntwrk/counter-cli run start-testnet-remote-ps",
    "test-against-testnet": "npm --workspace=@midnight-ntwrk/counter-cli run test-against-testnet",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "resolutions": {
    "@midnight-ntwrk/compact-runtime/@midnight-ntwrk/onchain-runtime": "^0.3.0",
    "@midnight-ntwrk/zswap": "^4.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@midnight-ntwrk/compact-runtime": "^0.8.1",
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "2.0.1",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "2.0.1",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "2.0.1",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "2.0.1",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "2.0.1",
    "@midnight-ntwrk/midnight-js-types": "2.0.1",
    "@midnight-ntwrk/wallet": "5.0.0",
    "@midnight-ntwrk/wallet-api": "5.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "@types/node": "^22.15.29",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "testcontainers": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "eslint": "^9.28.0",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "packageManager": "npm@11.4.1"
}
